class Solution:
    def isPalindrome(self, x: int) -> bool:
        sum=0
        val=x
        if x<0:
            return False
        else:
            while(x>0):
                 rem=x%10
                 sum=(sum*10)+rem
                 x=x//10
            if sum==val:
                return True
            return False

                 

          
            
